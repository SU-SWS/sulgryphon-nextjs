{
  "installCommand": "yarn workspaces focus --production",
  "rewrites": [
    {
      "source": "/all",
      "destination": "https://discover.stanford.edu/all"
    },
    {
      "source": "/all/:path*",
      "destination": "https://discover.stanford.edu/all/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "https://discover.stanford.edu/assets/:path*"
    }
  ]
}