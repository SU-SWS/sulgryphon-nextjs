{
  "buildCommand": "yarn build --no-lint",
  "installCommand": "yarn workspaces focus --production",
  "redirects": [
    {
      "source": "/sites/:path*",
      "destination": "/"
    },
    {
      "source": "/node/:path*",
      "destination": "/"
    },
    {
      "source": "/taxonomy/:path*",
      "destination": "/"
    },
    {
      "source": "/apcproxy/suproxy.pac",
      "destination": "/"
    },
    {
      "source": "/(.*).php",
      "destination": "/"
    },
    {
      "source": "/(.*).htm(.*)",
      "destination": "/"
    },
    {
      "source": "/(.*).sh",
      "destination": "/"
    },
    {
      "source": "/!",
      "destination": "/"
    },
    {
      "source": "/wp-content/:path*",
      "destination": "/"
    }
  ],
  "rewrites": [
    {
      "source": "/all",
      "destination": "https://discover.stanford.edu/all"
    },
    {
      "source": "/all/:path*",
      "destination": "https://discover.stanford.edu/all/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "https://discover.stanford.edu/assets/:path*"
    },
    {
      "source": "/catalog",
      "destination": "https://searchworks.stanford.edu/"
    },
    {
      "source": "/catalog/:path*",
      "destination": "https://searchworks.stanford.edu/:path*"
    },
    {
      "source": "/articles",
      "destination": "https://searchworks.stanford.edu/articles"
    },
    {
      "source": "/articles/:path*",
      "destination": "https://searchworks.stanford.edu/articles/:path*"
    }
  ]
}